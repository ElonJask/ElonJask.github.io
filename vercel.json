{
  "build": {
    "env": {
      "LANG": "en_US.UTF-8",
      "RUBYOPT": "-E utf-8"
    }
  },
  "headers": [
    {
      "source": "/images/posts/(.*)\\.(webp|png|jpg|jpeg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)\\.(ttf|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/topics",
      "destination": "/topics.html"
    },
    {
      "source": "/topics/",
      "destination": "/topics.html"
    },
    {
      "source": "/en/topics",
      "destination": "/en/topics.html"
    },
    {
      "source": "/en/topics/",
      "destination": "/en/topics.html"
    },
    {
      "source": "/archive",
      "destination": "/archive.html"
    },
    {
      "source": "/archive/",
      "destination": "/archive.html"
    },
    {
      "source": "/en/archive",
      "destination": "/en/archive.html"
    },
    {
      "source": "/en/archive/",
      "destination": "/en/archive.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/about/",
      "destination": "/about.html"
    },
    {
      "source": "/en/about",
      "destination": "/en/about.html"
    },
    {
      "source": "/en/about/",
      "destination": "/en/about.html"
    },
    {
      "source": "/stats/script.js",
      "destination": "https://cloud.umami.is/script.js"
    },
    {
      "source": "/stats/api/send",
      "destination": "https://cloud.umami.is/api/send"
    }
  ]
}
